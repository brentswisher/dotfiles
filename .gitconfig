[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
[user]
	name = swisherb
	email = swisherb@gvsu.edu
[color]
	ui = true
[color]
    branch = auto
    diff = auto
    status = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[push]
	default = simple
[alias]
        fs = !git fetch && git status
        hist = log --pretty=format:\"%ad - %an | %s%d \" --graph --date=relative
[filter "lfs"]
	clean = git lfs clean %f
	smudge = git lfs smudge %f
	required = true
